{
    "name": "jebog/tabler",
    "type": "package",
    "description": "Laravel Package for integrating Tabler template and this package is Laravel Mix friendly.",
    "keywords": [
        "laravel",
        "tabler",
        "package",
        "composer",
        "admin",
        "dashboard",
        "bootstrap 4"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^7.0 || >= 8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jebog\\Tabler\\Providers\\AppServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Jebog\\Tabler\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jebog\\Tabler\\Tests\\": "tests/"
        }
    }
}
